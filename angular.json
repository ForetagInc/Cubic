          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/schemas/jest.config.js",
            "tsConfig": "libs/schemas/tsconfig.spec.json",
            "setupFile": "libs/schemas/src/test-setup.ts"
          }
        }
      }
    },
